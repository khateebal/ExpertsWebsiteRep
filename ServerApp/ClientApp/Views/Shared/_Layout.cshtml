    <meta property="og:image" content="https://www.expertsconsult.co/images/logo/og-image.jpg" />
    @await RenderSectionAsync("metatags", required: false)
    <link rel="stylesheet" href="/assets/css/swiper-bundle.min.css">
    <link rel="stylesheet" href="/assets/css/style.css" asp-append-version="true">
    <link rel="stylesheet" href="/assets/css/fontawesome.css">
</head>
<body>
    @await Html.PartialAsync("_Header")
    @RenderBody()
    @await Html.PartialAsync("_Footer")

    <script src="/assets/js/jquery-3.6.0.min.js"></script>
    <script src="/assets/js/bootstrap.bundle.min.js"></script>
    <script src="/assets/js/typed.min.js"></script>
    <script src="~/assets/js/custom.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
    @await Html.PartialAsync("_TagsBody")
  
